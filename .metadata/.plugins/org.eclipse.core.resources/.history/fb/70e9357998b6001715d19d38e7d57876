		System.out.println("");
		System.out.println("");
		fleet.add(new Aircraft("N1762J", "Boeing", "777-300", 145538,  160520, 299370,892, 7500, 2, 396));
		fleet.add(new Aircraft("N1763J", "Airbus", "A333-300", 109185, 121870, 242000,871, 6000, 2, 440));
